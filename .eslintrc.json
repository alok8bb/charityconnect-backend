{
    "env": {
        "browser": false,
        "commonjs": true,
        "node": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "semi": ["error", "always"],
        "no-unused-vars": ["warn", { "args": "none" }]
    }
}
